<h1>Restaurant List</h1>
<% @restaurants.each do |restaurant| %>
  <h3><%= link_to restaurant.name, restaurant_path(restaurant.id)%>
  <%= link_to "Delete", restaurant_path(restaurant.id),
   data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}%></h3>
<% end %>
<p><%= link_to "Create restaurant", new_restaurant_path %></p>
